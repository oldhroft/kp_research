{
    "init_params": {
        "perceptron" : {
            "optimizer": "adam"
        },
        "lstm" : {
            "optimizer": "adam"
        },
        "gru" : {
            "optimizer": "adam"
        }
    },
    "param_grids": {
        "perceptron": {
            "units_array": [
                [16]
            ]
        },
        "lstm": {
            "units_array": {
                "rnn": [32],
                "dense": [32, 16]
            }
        },
        "gru": {
            "units_array": {
                "rnn": [32],
                "dense": [32, 16]
            }
        }
    },
    "scoring_params": {
        "average": "macro"
    },
    "seed": 16,
    "fit_params": {
        "perceptron": {
            "validation_split": 0.1,
            "epochs":  6,
            "verbose": 2
        },
        "lstm": {
            "validation_split": 0.1,
            "epochs":  1,
            "verbose": 2
        },
        "gru": {
            "validation_split": 0.1,
            "epochs":  1,
            "verbose": 2
        }
    },
    "callback_params": {
        "perceptron": {
            "monitor": "val_loss", 
            "restore_best_weights": true,
            "patience": 10
        }
    },
    "gv_params": {
        "verbose": 1,
        "seed": 17
    },
    "pipe_name": {
        "perceptron": "LagDataPipe",
        "lstm": "SequenceDataPipe",
        "gru": "SequenceDataPipe"
    },
    "pipe_params": {
        "perceptron": {
            "variables": [
                "Kp*10", "Dst", "B_x", "B_gsm_y", "B_gsm_z", 
                "B_magn", "SW_spd", "H_den_SWP", "doySin", "hourSin",
                "doyCos", "hourCos"],
            "scale": true,
            "shuffle": true,
            "target": "category",
            "backward_steps": 24, 
            "forward_steps": 8,
            "random_state": 16
        },
        "lstm": {
            "variables": [
                "Kp*10", "Dst", "B_x", "B_gsm_y", "B_gsm_z", 
                "B_magn", "SW_spd", "H_den_SWP", "doySin", "hourSin",
                "doyCos", "hourCos"],
            "scale": true,
            "shuffle": true,
            "target": "category",
            "backward_steps": 24, 
            "forward_steps": 8,
            "random_state": 16
        },
        "gru": {
            "variables": [
                "Kp*10", "Dst", "B_x", "B_gsm_y", "B_gsm_z", 
                "B_magn", "SW_spd", "H_den_SWP", "doySin", "hourSin",
                "doyCos", "hourCos"],
            "scale": true,
            "shuffle": true,
            "target": "category",
            "backward_steps": 24, 
            "forward_steps": 8,
            "random_state": 16
        }
    }
}